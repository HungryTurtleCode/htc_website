{
  "hosting": {
    "public": "_site",
    "headers": [{
      "source": "**/*.@(jpg|jpeg|gif|png|svg)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=30672000"
      }]
    },
    {
      "source": "**/*.@(css|js)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=31536000"
      }]
    }]
  }
}
